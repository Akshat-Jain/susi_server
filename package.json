{
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "1.13.7",
    "prettier-plugin-java": "0.0.10"
  },
  "scripts": {
    "precommit": "lint-staged",
    "format": "yarn prettier --write \"src/**/*.java\""
  },
  "lint-staged": {
    "src/**/*.java": [
      "yarn prettier --write \"src/**/*.java\"",
      "git add"
    ]
  }
}
